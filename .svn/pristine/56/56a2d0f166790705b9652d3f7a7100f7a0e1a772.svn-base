"use strict";

function addAddSectorListener() {
    var addSectorButton = document.getElementsByClassName("addSector");
    for (var i = 0; i < addSectorButton.length; i++)
        addSectorButton[i].addEventListener('click', () => {});

    addSectorButton = document.getElementById("addSector");
    addSectorButton.addEventListener("click", displayAddForm);
}

function addAddSectorFormListener() {
    var cancelSectorFormButton = document.getElementById("cancelSectorForm");
    cancelSectorFormButton.addEventListener("click", displayAdd);

    var confirmSectorForm = document.getElementById("confirmSectorForm");
    confirmSectorForm.addEventListener("click", displayAddAndSend);
}