<?php
class SectorDirectory 
{
    private $sectors = array();

    public function add($c)
    {
        array_push($this->courses, $c);
    }

    public function getSector($code)
    {
        $temp = null;

        foreach($this->sectors as $value)
        {
            if($value->getCode() == $code)
                $temp = $value;
        }

        return $temp;
    }

    public function getSectors() {
        return $this->sectors;
    }



    /**
     * @inheritDoc
     */
    public function jsonSerialize()
    {
        return ["sectors" => $this->sectors];
    }
}
